{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "sourceMap": true,
    "allowUnreachableCode": false,
    "strictPropertyInitialization": false,
    "experimentalDecorators": true,
    "lib": ["dom", "es2015.promise", "es5"]
  }
}
